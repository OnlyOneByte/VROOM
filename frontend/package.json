{
	"name": "vroom-frontend",
	"private": true,
	"version": "1.0.0",
	"description": "VROOM Car Tracker Frontend PWA",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
